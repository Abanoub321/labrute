{
  "name": "@eternaltwin/labrute-server",
  "version": "0.3.14",
  "private": true,
  "entry": "server/server.js",
  "type": "module",
  "dependencies": {
    "@eternal-twin/core": "^0.10.3",
    "@eternal-twin/etwin-client-http": "^0.10.3",
    "@mui/lab": "=5.0.0-alpha.75",
    "@mui/material": "^5.9.2",
    "body-parser": "^1.20.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "pg": "^8.7.3",
    "semver-sort": "^1.0.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "lint": "eslint \"./src/**/*.ts\"",
    "format": "eslint --fix \"./src/**/*.ts\"",
    "release": "standard-version -a",
    "front": "cd client && npm run start",
    "back": "nodemon ./src/server.ts",
    "build": "yarn run build:assets && yarn run build:scripts",
    "build:assets": "cpy \"./src/db/migrations/*.sql\" \"./build/db/migrations/\"",
    "build:scripts": "tsc -p ./tsconfig.json",
    "start": "ts-node --esm ./src/server.ts",
    "db:sync": "ts-node --esm ./src/db/sync-database.ts"
  },
  "devDependencies": {
    "@eternal-twin/cli": "^0.10.3",
    "@types/express": "^4.17.13",
    "@types/node": "^18.6.2",
    "@types/pg": "^8.6.5",
    "@types/semver-sort": "^0.0.1",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "cpy-cli": "^4.1.0",
    "eslint": "^8.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-tsdoc": "^0.2.16",
    "nodemon": "^2.0.19",
    "standard-version": "^9.5.0",
    "typescript": "^4.7.4"
  },
  "workspaces": {
    "packages": [
      "client",
      "server"
    ]
  },
  "packageManager": "yarn@3.2.2"
}
