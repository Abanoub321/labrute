{
  "name": "labrute",
  "version": "2.0.0",
  "private": true,
  "engines": {
    "npm": ">=8.1.2",
    "node": ">=16.13.1"
  },
  "scripts": {
    "clean": "rimraf \"client/build\" \"core/lib\" \"server/lib\" \"**/node_modules\" \"**/tsconfig.tsbuildinfo\"",
    "compile": "tsc -b tsconfig.build.json",
    "front": "npm run start --workspace=client",
    "back": "npm run start:watch --workspace=server",
    "etwin": "etwin",
    "start": "npm run start --workspace=server",
    "dev": "concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT,ETWIN\" -c \"bgBlue.bold,bgGreen.bold,bgYellow.bold\" \"npm run back\" \"npm run front\" \"npm run etwin\"",
    "db:sync:dev": "npm run db:sync:dev --workspace=server",
    "db:sync:prod": "npm run db:sync:prod --workspace=server",
    "db:seed": "npm run db:seed --workspace=server",
    "db:reset": "npm run db:reset --workspace=server",
    "postinstall": "bash ./postInstall.sh",
    "animation:import": "ts-node importAnimation.ts"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4"
  },
  "workspaces": [
    "client",
    "core",
    "prisma",
    "server"
  ],
  "dependencies": {
    "@eternal-twin/cli": "^0.10.3"
  }
}
